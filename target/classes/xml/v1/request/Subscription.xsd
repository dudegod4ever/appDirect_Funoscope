<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="subscription">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="order">
          <xs:complexType>
            <xs:sequence>
              <xs:element type="xs:integer" name="paymentPlanId"/>
              <xs:element type="xs:integer" name="discountId"/>
              <xs:element name="orderLines">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="orderLine">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="unit"/>
                          <xs:element type="xs:integer" name="quantity"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>